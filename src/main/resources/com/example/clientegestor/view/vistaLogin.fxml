<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clientegestor.controller.vistaLoginController">
   <children>
      <TextField fx:id="tfIdUsuario" layoutX="991.0" layoutY="293.0" prefHeight="25.0" prefWidth="195.0" />
      <Label layoutX="923.0" layoutY="297.0" text="User" />
      <Label layoutX="904.0" layoutY="352.0" text="Password" />
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="1280.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="MIClose" mnemonicParsing="false" onAction="#onClose" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="btnLogin" layoutX="991.0" layoutY="412.0" mnemonicParsing="false" onAction="#onLogin" text="Log In" />
      <Button fx:id="btnSignIn" layoutX="1134.0" layoutY="412.0" mnemonicParsing="false" onAction="#OnSignIn" text="Sign In" />
      <TextField fx:id="tfIDPassword" layoutX="991.0" layoutY="348.0" prefHeight="25.0" prefWidth="195.0" />
   </children>
</AnchorPane>
